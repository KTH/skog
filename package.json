    "@types/cls-hooked": "^4.3.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/pino": "^6.3.11",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "eslint": "^7.30.0",
    "jest": "^27.2.4",
